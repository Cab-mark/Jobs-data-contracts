{
  "name": "jobs-data-contracts",
  "version": "1.2.2",
  "description": "Canonical data contracts and schemas for the CS Jobs Board solution. This repository serves as the single source of truth for all data structures used across the Jobs Board ecosystem.",
  "main": "./dist/typescript/src/index.js",
  "module": "./dist/typescript/src/index.js",
  "types": "./dist/typescript/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/typescript/src/index.d.ts",
      "import": "./dist/typescript/src/index.js",
      "require": "./dist/typescript/src/index.js"
    },
    "./jobs": {
      "types": "./dist/typescript/src/jobs.d.ts",
      "import": "./dist/typescript/src/jobs.js",
      "require": "./dist/typescript/src/jobs.js"
    },
    "./search": {
      "types": "./dist/typescript/src/search.d.ts",
      "import": "./dist/typescript/src/search.js",
      "require": "./dist/typescript/src/search.js"
    }
  },
  "files": [
    "dist/typescript",
    "dist/python",
    "schemas",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "generate:jobs": "openapi-typescript ./schemas/jobs/openapi.yaml -o ./generated/typescript/jobs.ts",
    "generate:search": "openapi-typescript ./schemas/search/openapi.yaml -o ./generated/typescript/search.ts",
    "generate:src": "node scripts/generate-src-files.js",
    "generate:typescript": "npm run generate:jobs && npm run generate:search && npm run generate:src",
    "generate:python": "./scripts/generate_pydantic.sh",
    "build": "npm run generate:typescript && tsc",
    "build:python": "python -m build --outdir dist/python",
    "prepublishOnly": "npm run build && npm run build:python",
    "test": "tsc --noEmit"
  },
  "keywords": [
    "jobs",
    "data-contracts",
    "typescript",
    "openapi",
    "types"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "openapi-typescript": "^7.10.1",
    "typescript": "^5.9.3"
  }
}
